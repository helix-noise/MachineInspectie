<Application
    x:Class="MachineInspectie.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MachineInspectie"
    xmlns:interop="using:Windows.UI.Xaml.Interop">

    <Application.Resources>
        <ResourceDictionary>
            <!-- Overriden theme resources -->
            <SolidColorBrush x:Key="ButtonDisabledBorderThemeBrush" Color="#FF06428E"/>
            <SolidColorBrush x:Key="ButtonDisabledBackgroundThemeBrush" Color="DarkGray"/>
            <SolidColorBrush x:Key="ButtonPressedBackgroundThemeBrush" Color="Transparent" />
            <SolidColorBrush x:Key="TextBoxBorderThemeBrush" Color="#FF06428E" />
        </ResourceDictionary>
    </Application.Resources>

</Application>
    